#################################################################
#    Classes
#################################################################

###  http://www.estudiantes.org/LibreriaUNAL#GeneroCompuesto
:GeneroCompuesto rdf:type owl:Class ;
                 rdfs:subClassOf :Libro .


###  http://www.estudiantes.org/LibreriaUNAL#Ilustracion
:Ilustracion rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :tieneAutor ;
                               owl:allValuesFrom :Autor
                             ] .


###  http://www.estudiantes.org/LibreriaUNAL#Ilustrado
:Ilustrado rdf:type owl:Class ;
           rdfs:subClassOf :Publicacion ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :tieneIlustracion ;
                             owl:someValuesFrom :Ilustracion
                           ] ;
           owl:disjointWith :NoIlustrado .


###  http://www.estudiantes.org/LibreriaUNAL#NoIlustrado
:NoIlustrado rdf:type owl:Class ;
             rdfs:subClassOf :Publicacion .


###  http://www.estudiantes.org/LibreriaUNAL#RomanceHistorico
:RomanceHistorico rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :GeneroCompuesto
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :esGenero ;
                                                               owl:allValuesFrom [ owl:intersectionOf ( :Historia
                                                                                                        :Romantica
                                                                                                      ) ;
                                                                                   rdf:type owl:Class
                                                                                 ]
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  owl:disjointWith :Tragicomedia .


###  http://www.estudiantes.org/LibreriaUNAL#Tragicomedia
:Tragicomedia rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :GeneroCompuesto
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :esGenero ;
                                                           owl:allValuesFrom [ owl:intersectionOf ( :Comedia
                                                                                                    :Tragedia
                                                                                                  ) ;
                                                                               rdf:type owl:Class
                                                                             ]
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] .


###  http://www.estudiantes.org/LibreriaUNAL#jamon
:jamon rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty :tieneIlustracion ;
                                                    owl:someValuesFrom :Ilustracion
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :tieneAutor ;
                                                    owl:someValuesFrom :Autor
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :tieneEditorial ;
                                                    owl:someValuesFrom :Editorial
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :tieneGenero ;
                                                    owl:someValuesFrom :Cientifico
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] .


