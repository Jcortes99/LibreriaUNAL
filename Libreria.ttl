###  http://www.estudiantes.org/LibreriaUNAL/Libro
:Libro rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :tieneAutor ;
                         owl:someValuesFrom :Autor
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :tieneEditorial ;
                         owl:someValuesFrom :Editorial
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :tieneGenero ;
                         owl:someValuesFrom :Genero
                       ] .


###  http://www.estudiantes.org/LibreriaUNAL/Persona
:Persona rdf:type owl:Class .


###  http://www.estudiantes.org/LibreriaUNAL/Publicacion
:Publicacion rdf:type owl:Class ;
             rdfs:subClassOf :Libro ;
             rdfs:label "Trabajos publicados" .


