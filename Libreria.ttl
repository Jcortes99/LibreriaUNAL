###  http://www.estudiantes.org/LibreriaUNAL#distribuye
:distribuye rdf:type owl:ObjectProperty ;
            owl:equivalentProperty :esEditorial ;
            owl:inverseOf :esDistribuido ;
            rdfs:domain :Editorial ;
            rdfs:range :Libro .


###  http://www.estudiantes.org/LibreriaUNAL#esDistribuido
:esDistribuido rdf:type owl:ObjectProperty ;
               owl:equivalentProperty :tieneEditorial ;
               rdfs:domain :Libro ;
               rdfs:range :Editorial .


###  http://www.estudiantes.org/LibreriaUNAL#tieneIlustracion
:tieneIlustracion rdf:type owl:ObjectProperty ;
                  rdfs:domain :Libro ;
                  rdfs:range :Ilustracion .


###  http://www.estudiantes.org/LibreriaUNAL/esAutor
:esAutor rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :tieneAutor ;
         rdfs:domain :Autor ;
         rdfs:range :Libro .


###  http://www.estudiantes.org/LibreriaUNAL/esEditorial
:esEditorial rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :tieneEditorial ;
             rdf:type owl:InverseFunctionalProperty ;
             rdfs:domain :Editorial ;
             rdfs:range :Libro .


###  http://www.estudiantes.org/LibreriaUNAL/esGenero
:esGenero rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf :tieneGenero ;
          rdfs:domain :Genero ;
          rdfs:range :Libro .


###  http://www.estudiantes.org/LibreriaUNAL/publicadoAntes
:publicadoAntes rdf:type owl:ObjectProperty ;
