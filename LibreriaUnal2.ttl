@prefix : <http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@base <http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/> .

<http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/amigoDe
:amigoDe rdf:type owl:ObjectProperty ,
                  owl:SymmetricProperty ;
         rdfs:domain :Socio ;
         rdfs:range :Socio ;
         rdfs:label "Amigo de" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/antesDe
:antesDe rdf:type owl:ObjectProperty ;
         owl:inverseOf :despuesDe ;
         rdf:type owl:TransitiveProperty ;
         rdfs:domain :Libro ;
         rdfs:range :Libro ;
         rdfs:label "antes de" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/autorDe
:autorDe rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :tieneAutor ;
         rdf:type owl:InverseFunctionalProperty ;
         rdfs:domain :Autor ;
         rdfs:range :Libro ;
         rdfs:label "autor de" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/despuesDe
:despuesDe rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdf:type owl:TransitiveProperty ;
           rdfs:domain :Libro ;
           rdfs:range :Libro ;
           rdfs:label "despues de" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/editorDe
:editorDe rdf:type owl:ObjectProperty ;
          owl:inverseOf :tieneEditor ;
          rdfs:domain :Editor ;
          rdfs:range :PublicacionPeriodica ;
          rdfs:label "editor de" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/editorialDe
:editorialDe rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :tieneEditorial ;
             rdfs:domain :Editorial ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Libro
                                        :MaterialIlustrado
                                      )
                        ] ;
             rdfs:label "editorial de" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/generoDe
:generoDe rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :Genero ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Libro
                                     :MaterialIlustrado
                                   )
                     ] ;
          rdfs:label "genero de" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/mangakaDe
:mangakaDe rdf:type owl:ObjectProperty ;
           owl:inverseOf :tieneMangaka ;
           rdf:type owl:InverseFunctionalProperty ;
           rdfs:domain :Mangaka ;
           rdfs:range :Manga ;
           rdfs:label "mangaka de" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/prestadoA
:prestadoA rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Libro ;
           rdfs:range :Socio ;
           rdfs:label "prestado a" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/superheroeDe
:superheroeDe rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :tieneSuperheroe ;
              rdfs:domain :Superheroe ;
              rdfs:range :Comic ;
              rdfs:label "superheroe de" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneAutor
:tieneAutor rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Libro ;
            rdfs:range :Autor ;
            rdfs:label "tiene autor" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneEditor
:tieneEditor rdf:type owl:ObjectProperty ;
             rdfs:domain :PublicacionPeriodica ;
             rdfs:range :Editor ;
             rdfs:label "tiene editor" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneEditorial
:tieneEditorial rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Libro
                                            :MaterialIlustrado
                                          )
                            ] ;
                rdfs:range :Editorial ;
                rdfs:label "tiene editorial" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneGenero
:tieneGenero rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Libro
                                         :MaterialIlustrado
                                       )
                         ] ;
             rdfs:range :Genero ;
             rdfs:label "tiene genero" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneMangaka
:tieneMangaka rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Manga ;
              rdfs:range :Mangaka ;
              rdfs:label "tiene mangaka" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneSuperheroe
:tieneSuperheroe rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :Comic ;
                 rdfs:range :Superheroe ;
                 rdfs:label "tiene superheroe" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/ubicadoEn
:ubicadoEn rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Libro
                                       :MaterialIlustrado
                                       :PublicacionPeriodica
                                     )
                       ] ;
           rdfs:range :Ubicacion ;
           rdfs:label "buscado en" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/añoDeNacimiento
:añoDeNacimiento rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain :Persona ;
                 rdfs:range xsd:int .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneEdicion
:tieneEdicion rdf:type owl:DatatypeProperty ;
              rdfs:domain :Revista ;
              rdfs:range xsd:string .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneFechaPublicacion
:tieneFechaPublicacion rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Periodico ;
                       rdfs:range xsd:int .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneIdentificador
:tieneIdentificador rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Socio ;
                    rdfs:range xsd:int .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneNombre
:tieneNombre rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Editorial
                                         :Genero
                                         :Persona
                                         :PublicacionPeriodica
                                         :Superheroe
                                         :Ubicacion
                                       )
                         ] ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneNumeroPublicacion
:tieneNumeroPublicacion rdf:type owl:DatatypeProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :Comic
                                                    :Manga
                                                  )
                                    ] ;
                        rdfs:range xsd:int .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/tieneTitulo
:tieneTitulo rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Libro ;
             rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Autor
:Autor rdf:type owl:Class ;
       rdfs:subClassOf :Persona ;
       rdfs:label "Autor" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Comic
:Comic rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :MaterialIlustrado
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :tieneSuperheroe ;
                                                    owl:someValuesFrom :Superheroe
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :tieneNumeroPublicacion ;
                                                    owl:someValuesFrom xsd:int
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:label "Comic" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Disponible
:Disponible rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Libro
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :ubicadoEn ;
                                                         owl:someValuesFrom :Ubicacion
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:label "LibroDisponible" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Editor
:Editor rdf:type owl:Class ;
        rdfs:subClassOf :Persona ;
        rdfs:label "Editor" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Editorial
:Editorial rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :tieneNombre ;
                             owl:someValuesFrom xsd:string
                           ] ;
           rdfs:label "Editoral" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Genero
:Genero rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :tieneNombre ;
                          owl:someValuesFrom xsd:string
                        ] ;
        rdfs:label "Genero" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Libro
:Libro rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :tieneAutor ;
                         owl:someValuesFrom :Autor
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :tieneEditorial ;
                         owl:someValuesFrom :Editorial
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :tieneGenero ;
                         owl:someValuesFrom :Genero
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :tieneTitulo ;
                         owl:someValuesFrom xsd:string
                       ] ;
       rdfs:label "Libro" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Manga
:Manga rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :MaterialIlustrado
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :tieneMangaka ;
                                                    owl:someValuesFrom :Mangaka
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :tieneNumeroPublicacion ;
                                                    owl:someValuesFrom xsd:int
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:label "Manga" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Mangaka
:Mangaka rdf:type owl:Class ;
         rdfs:subClassOf :Persona ;
         rdfs:label "Mangaka" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/MaterialIlustrado
:MaterialIlustrado rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :tieneEditorial ;
                                     owl:someValuesFrom :Editorial
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :tieneGenero ;
                                     owl:someValuesFrom :Genero
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :ubicadoEn ;
                                     owl:someValuesFrom :Ubicacion
                                   ] ;
                   rdfs:label "Material Ilustrado" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Periodico
:Periodico rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :PublicacionPeriodica
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :tieneFechaPublicacion ;
                                                        owl:someValuesFrom xsd:int
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:label "Periodico" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Persona
:Persona rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :añoDeNacimiento ;
                           owl:someValuesFrom xsd:int
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :tieneNombre ;
                           owl:someValuesFrom xsd:string
                         ] ;
         rdfs:label "Persona";
         owl:equivalentClass foaf:Person.


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Prestado
:Prestado rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Libro
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :prestadoA ;
                                                       owl:someValuesFrom :Socio
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:label "LibroPrestado" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/PublicacionPeriodica
:PublicacionPeriodica rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty :tieneEditor ;
                                        owl:someValuesFrom :Editor
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :ubicadoEn ;
                                        owl:someValuesFrom :Ubicacion
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :tieneNombre ;
                                        owl:someValuesFrom xsd:string
                                      ] ;
                      rdfs:label "Publicacion periodica" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Revista
:Revista rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :PublicacionPeriodica
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :tieneEdicion ;
                                                      owl:someValuesFrom xsd:string
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:label "Revista" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Socio
:Socio rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Persona
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :tieneIdentificador ;
                                                    owl:someValuesFrom xsd:int
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:label "Socio" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Superheroe
:Superheroe rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :tieneNombre ;
                              owl:someValuesFrom xsd:string
                            ] ;
            rdfs:label "SuperHeroe" .


###  http://www.semanticweb.org/Biblioteca-Hemeroteca-Ludoteca/Ubicacion
:Ubicacion rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :tieneNombre ;
                             owl:someValuesFrom xsd:string
                           ] ;
           rdfs:label "Ubicacion" .


